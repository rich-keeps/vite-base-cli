{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": ["plugin:vue/vue3-essential", "airbnb-base"],
  "overrides": [],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "plugins": ["vue"],
  "rules": {
    "quotes": ["error", "double"],
    "import/no-unresolved": "off",
    "import/no-extraneous-dependencies": "off",
    "vue/multi-word-component-names": "off",
    "comma-dangle": "off",
    "no-unused-expressions": "off",
    "no-param-reassign": "off"
  }
}
